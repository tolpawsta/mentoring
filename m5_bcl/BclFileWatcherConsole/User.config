<?xml version="1.0" encoding="utf-8" ?>
<appSection appName="FileWatcherConsole">
	<culture name="en-US"/>
	<directories>
		<dir path="d:\example"/>
		<dir path="d:\example"/>
		<dir path="d:\exampleNew"/>
	</directories>
	<rules>
		<rule fileNamePattern="\w*1.txt" pathDirMoveTo="d:\example\TextFiles" shoudAddCounter="true" shoudAddMoveDate="true"/>
		<rule fileNamePattern="\w*.jpg" pathDirMoveTo="d:\example\Images" shoudAddCounter="false" shoudAddMoveDate="false"/>
	</rules>
	<defaultDirectory path="d:\example\temp"/>
</appSection>